{
  "name": "remedy",
  "version": "3.0.1",
  "description": "emitting middleware for file removing",
  "main": "server/index.js",
  "scripts": {
    "test": "tape test/**/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "lint": "redrun lint:*",
    "lint:client": "eslint --env browser --rule 'no-console:0' client",
    "lint:server": "eslint server",
    "build": "redrun build:*",
    "build:client": "webpack --progress",
    "build:client:dev": "NODE_ENV=development redrun build:client",
    "watcher": "nodemon -w client -w server --exec",
    "watch:test": "npm run watcher -- 'npm test'",
    "watch:lint": "npm run watcher -- 'npm run lint'",
    "watch:client": "redrun build:client -- --watch",
    "watch:client:dev": "redrun build:client:dev -- --watch",
    "wisdom": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-remedy.git"
  },
  "keywords": [
    "file",
    "remove",
    "emitter",
    "middleware"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-remedy/issues"
  },
  "homepage": "https://github.com/coderaiser/node-remedy",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "mellow": "~2.0.0",
    "remy": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^3.0.0",
    "emitify": "^3.1.0",
    "eslint": "^4.1.1",
    "express": "^4.14.0",
    "mkdirp": "^0.5.1",
    "nyc": "^11.0.2",
    "redrun": "^5.0.1",
    "rimraf": "^2.6.1",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "tape": "^4.6.0",
    "webpack": "^3.1.0"
  }
}

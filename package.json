{
  "name": "remedy",
  "version": "1.5.9",
  "description": "emitting middleware for file removing",
  "main": "lib/server.js",
  "scripts": {
    "test": "tape test/**/*.js",
    "watch:test": "nodemon -w test -w lib --exec npm test",
    "lint": "redrun lint:*",
    "lint:eslint:client": "eslint --config .eslintrc-client lib --ignore-pattern lib/server",
    "lint:eslint:server": "eslint lib test --ignore-pattern lib/client.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-remedy.git"
  },
  "keywords": [
    "file",
    "remove",
    "emitter",
    "middleware"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-remedy/issues"
  },
  "homepage": "https://github.com/coderaiser/node-remedy",
  "dependencies": {
    "join-io": "~1.4.0",
    "mellow": "~2.0.0",
    "mollify": "~1.0.4",
    "remy": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.11",
    "eslint": "^4.0.0",
    "express": "^4.14.0",
    "nyc": "^11.0.2",
    "redrun": "^5.0.1",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "tape": "^4.6.0"
  }
}
